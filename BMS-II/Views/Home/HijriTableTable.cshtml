</thead>
<tbody data-page="@(hijriTableList.PageNumber)">
@{
    await hijriTableList.SetupGrid();
    bool eof = hijriTableList.Recordset == null; // DN
}
@while (hijriTableList.RecordCount < hijriTableList.StopRecord || SameString(hijriTableList.RowIndex, "$rowindex$")) { // Begin While
    if (!eof && !SameString(hijriTableList.RowIndex, "$rowindex$") &&
