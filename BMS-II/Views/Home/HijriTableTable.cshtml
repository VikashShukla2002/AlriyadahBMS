@if (hijriTableList.ModalGridAdd && hijriTableList.IsGridAdd || hijriTableList.ModalGridEdit && hijriTableList.IsGridEdit || hijriTableList.ModalMultiEdit && hijriTableList.IsMultiEdit) {
<script>
var currentTable = @Html.Raw(ConvertToJson(hijriTableList.ToClientVar()));
ew.deepAssign(ew.vars, { tables: { Hijri_Table: currentTable } });
var currentPageID = ew.PAGE_ID = "list";
var currentForm;
var @(hijriTableList.FormName);
loadjs.ready(["wrapper", "head"], function () {
    let $ = jQuery;
    let fields = currentTable.fields;

    // Form object
    let form = new ew.FormBuilder()
        .setId("@(hijriTableList.FormName)")
        .setPageId("list")
        .setSubmitWithFetch(@(hijriTableList.UseAjaxActions ? "true" : "false"))
        .setFormKeyCountName("@(hijriTableList.FormKeyCountName)")
        .build();
    window[form.id] = form;
    currentForm = form;
    loadjs.done(form.id);
});
</script>
<script>
