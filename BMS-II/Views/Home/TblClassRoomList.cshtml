        .setLists({
            "int_CR_Product_ID": @Html.Raw(tblClassRoomList.int_CR_Product_ID.ToClientList(tblClassRoomList)),
            "mon_CR_Price": @Html.Raw(tblClassRoomList.mon_CR_Price.ToClientList(tblClassRoomList)),
