            "int_Teacher_ID": @Html.Raw(tblClassRoomList.int_Teacher_ID.ToClientList(tblClassRoomList)),
        })
        .build();
    window[form.id] = form;
    currentForm = form;
    loadjs.done(form.id);
});
</script>
