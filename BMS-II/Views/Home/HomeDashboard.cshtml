        <h3 class="card-title">@Html.Raw(Language.TablePhrase("Registration_Status", "TblCaption"))</h3>
@if (!homeDashboard.IsExport()) {
        <div class="card-tools">
    @if (homeDashboard.CanRefresh) {
            <button type="button" class="btn btn-tool" data-card-widget="card-refresh" data-source="@AppPath("RegistrationStatus")?layout=false&dashboard=true" data-load-on-init="@(homeDashboard.LoadOnInit ? "true" : "false")"><i class="fa-solid fa-rotate"></i></button>
    }
    @if (homeDashboard.CanMaximize) {
            <button type="button" class="btn btn-tool" data-card-widget="maximize"><i class="fa-solid fa-maximize"></i></button>
    }
    @if (homeDashboard.CanCollapse) {
            <button type="button" class="btn btn-tool" data-card-widget="collapse"><i class="fa-solid fa-minus"></i></button>
    }
        </div>
}
    </div><!-- /.card-header -->
