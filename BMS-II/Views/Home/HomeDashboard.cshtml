@{
    // View
    View = this;

    // Check action result
    if (homeDashboard.ActionResult is EmptyResult) { // Terminate page
        return;
    }

    // Layout
    Layout = "_Layout";
    HomeDashboard DashboardPage = homeDashboard;

    // Local variables (Temp data)
    #pragma warning disable 168
    string? sqlWrk, filterWrk, whereWrk, curVal, dispVal;
    List<Dictionary<string, object>>? rswrk;
    string[]? arWrk;
    List<object?>? listWrk;
    object? guidWrk;
    Func<string>? lookupFilter;
    #pragma warning restore 168
}
<script>
var currentTable = @Html.Raw(ConvertToJson(homeDashboard.ToClientVar()));
ew.deepAssign(ew.vars, { tables: { Home_Dashboard: currentTable } });
var currentPageID = ew.PAGE_ID = "dashboard";
var currentForm;
var fHome_Dashboarddashboard;
loadjs.ready(["wrapper", "head"], function () {
    let $ = jQuery;
    let fields = currentTable.fields;

    // Form object
    let form = new ew.FormBuilder()
        .setId("fHome_Dashboarddashboard")
        .setPageId("dashboard")
        .build();
    window[form.id] = form;
    currentForm = form;
    loadjs.done(form.id);
});
</script>
<script>
loadjs.ready("head", function() {
    // Write your client script here, no need to add script tags.
});
</script>
<!-- Content Container -->
<div id="ew-report" class="ew-report">
<div class="btn-toolbar ew-toolbar">
@{ homeDashboard.ExportOptions.Render("body"); }
</div>
@homeDashboard.ShowPageHeader()
@homeDashboard.ShowMessages()
