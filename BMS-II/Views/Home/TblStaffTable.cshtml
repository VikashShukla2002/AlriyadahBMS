} @* tblStaffList.Absherphoto.Visible (end) *@
@* Render list options (header, right) *@
@tblStaffList.ListOptions.RenderHeader("right")
    </tr>
</thead>
<tbody data-page="@(tblStaffList.PageNumber)">
@{
    await tblStaffList.SetupGrid();
    bool eof = tblStaffList.Recordset == null; // DN
}
@while (tblStaffList.RecordCount < tblStaffList.StopRecord || SameString(tblStaffList.RowIndex, "$rowindex$")) { // Begin While
