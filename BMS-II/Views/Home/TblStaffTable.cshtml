        eof = !await tblStaffList.Recordset.ReadAsync();
    tblStaffList.RecordCount++;
    if (tblStaffList.RecordCount >= tblStaffList.StartRecord) {
        await tblStaffList.SetupRow();
    <tr @Html.Raw(tblStaffList.RowAttributes)>
