    // Reset for template row
    if (SameString(tblStaffList.RowIndex, "$rowindex$"))
        tblStaffList.RowIndex = 0;

    // Reset inline add/copy row
    if ((tblStaffList.IsCopy || tblStaffList.IsAdd) && tblStaffList.RowIndex is int rowIndex2 && rowIndex2 == 0)
        tblStaffList.RowIndex = 1;
